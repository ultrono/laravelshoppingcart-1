{
    "name": "ultrono/laravelshoppingcart-1",
    "description": "Laravel 11 and above Shopping cart",
    "keywords": ["laravel", "shopping cart", "cart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Darryl Fernandez",
            "email": "engrdarrylfernandez@gmail.com"
        },
        {
            "name": "Rob Allport",
            "email": "rob@f9web.co.uk"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/support": "^11.0",
        "illuminate/validation": "^11.0",
        "illuminate/translation": "^11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^9.5"
    },
    "autoload": {
        "psr-4": {
            "Darryldecode\\": "src/Darryldecode"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Darryldecode\\Cart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Darryldecode\\Cart\\Facades\\CartFacade"
            }
        }
    }
}
